    "@babel/plugin-proposal-object-rest-spread": "^7.13.0",
    "@babel/preset-env": "^7.13.5",
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "ansi-colors": "^4.1.1",
    "autoprefixer": "10.2.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
