{
  "name": "frontenso-11ty-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all clean -p dev:*",
    "dev:gulp": "gulp&",
    "dev:eleventy": "eleventy --serve --port=9000",
    "dev:webpack": "webpack --watch",
    "build": "cross-env NODE_ENV=production npm-run-all clean -p build:*",
    "build:gulp": "gulp production",
    "build:eleventy": "eleventy",
    "build:webpack": "webpack",
    "clean": "rimraf dist/",
    "zip": "cross-env NODE_ENV=production gulp zip",
    "format": "pretty-quick --staged",
    "lint:css": "stylelint \"src/**/*.{scss,sass}\"",
    "lint:js": "eslint --max-warnings 0 \"src/**/*.{jsx,js}\"",
    "validate": "npm-run-all --parallel format lint:css lint:js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run validate"
    }
  },
  "author": "einomi",
  "license": "ISC",
